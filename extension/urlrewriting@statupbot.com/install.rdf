<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>urlrewriting@statupbot.com</em:id>
        <em:version>0.3</em:version>
        <em:type>2</em:type>
        <em:bootstrap>true</em:bootstrap>
        <em:name>Url Rewriting</em:name>
        <em:description>Url Rewriting for Statupbot</em:description>
        <em:creator>Dave Scrapper</em:creator>
        <em:unpack>true</em:unpack>

        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.0</em:minVersion>
                <em:maxVersion>25.*</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- Platforms where we are -->
        <em:targetPlatform>WINNT_x86-msvc</em:targetPlatform>
        <em:targetPlatform>Linux</em:targetPlatform>

        <!-- We're probably missing lots of platforms here -->
    </Description>

</RDF>
